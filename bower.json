{
	"private": true,
  "name": "blackledge",
  "version": "2.1.0",
  "description": "Generic, reusable front end component framework.",
  "homepage": "https://github.com/furzeface/blackledge#readme",
  "authors": [
    "Daniel Furze <daniel@furzeface.com>",
    "Andrew 'The Ledge' Blackledge <blackledge22@gmail.com>"
  ],
  "license": "http://danielfurze.mit-license.org",
  "ignore": [
    "**/.*",
    "bower_components",
    "node_modules"
  ],
  "devDependencies": {
    "conformity": "~0.2.3",
    "console-polyfill": "~0.1.2",
    "fitvids": "~1.1.0",
    "jquery": "1.11.1",
    "jquery-smartresize": "*",
    "jquery-tiny-pubsub": "~0.7.0",
    "modernizr": "~2.8.2",
    "normalize-scss": "~3.0.3",
    "OwlCarousel": "https://github.com/smashingboxes/OwlCarousel2.git",
    "picturefill": "~2.3.1"
  },
  "preen": {
  	"conformity": [
  		"dist/conformity.min.js"
  	],
    "console-polyfill": [
      "*.js"
    ],
    "fitvids": [
    	"*.js"
    ],
    "jquery": [
      "dist/*.{js,map}"
    ],
    "jquery-smartresize": [
    	"*.js"
    ],
    "jquery-tiny-pubsub": [
      "dist/*.min.js"
    ],
    "modernizr": [
      "modernizr.js"
    ],
    "normalize-scss": [
      "_normalize.scss"
    ],
    "OwlCarousel": [
    "src/js/*.js"
    ],
    "picturefill": [
    	"dist/*.min.js"
    ]
  }
}
